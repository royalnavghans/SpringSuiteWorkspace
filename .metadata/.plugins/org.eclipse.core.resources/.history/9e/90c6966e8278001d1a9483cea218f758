package javaBasicsQues;

public class Arr {
 public static void main (String[] args) {
	 int arr[] = {5, 2};
	
	 int min;
	 int max;
	 for(int i=0;i<arr.length;i++) {
		 System.out.println(arr[i]);
		 if(arr[0]<arr[1]) {
			 System.out.println(arr[0]);
		 }else {
			 System.out.println(arr[1]);
		 }
	 }
	 
 }
}
